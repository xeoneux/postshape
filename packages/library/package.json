{
  "name": "postshape",
  "version": "0.0.0-development",
  "description": "A minimal design system and library of composable React components, primarily for my personal projects.",
  "repository": "git@github.com:xeoneux/postshape.git",
  "author": "Harry Hogg <harry@hogg.io>",
  "license": "MIT",
  "source": "src",
  "type": "module",
  "main": "./dist/postshape.es.js",
  "module": "./dist/postshape.es.js",
  "types": "./dist/index.d.ts",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite build --watch",
    "build": "bun run clean && tsc && vite build && bun run build:docs",
    "build:docs": "typedoc src/index.ts --exclude lib --excludeExternals --json ../website/src/assets/documentation.json --jsDocCompatibility false",
    "tsc:check": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.4",
    "classnames": "^2.3.2",
    "framer-motion": "^10.16.16",
    "lodash.omit": "^4.5.0",
    "lucide-react": "^0.294.0",
    "luxon": "^3.4.4",
    "normalize.css": "^8.0.1",
    "open-color": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "resize-observer-polyfill": "^1.5.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/classnames": "^2.3.1",
    "@types/lodash.omit": "^4.5.9",
