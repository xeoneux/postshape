{
  "dependencies": {
    "@artsy/fresnel": "^7.1.3",
    "lodash.omit": "^4.5.0",
    "lucide-react": "^0.294.0",
    "luxon": "^3.4.4",
    "next": "^14.0.3",
    "postshape": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typedoc": "^0.24.8"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/lodash.omit": "^4.5.9",
    "@types/luxon": "^3.3.6",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.39",
