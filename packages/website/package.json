{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "next build",
    "build:svgs": "bun run build:svgs:pictograms && bun run build:svgs:logo",
    "build:svgs:logo": "bun run svgr ./src/assets/postshape-icon.svg --out-dir ./src/components/Logo --ext tsx --config-file .svgrrc.logo.cjs",
    "build:svgs:pictograms": "bun run svgr -d ./src/docs/catalog/pictograms ./src/assets/pictogram-svgs --ext tsx --config-file .svgrrc.pictograms.cjs"
  },
  "dependencies": {
    "@artsy/fresnel": "^7.1.3",
    "lodash.omit": "^4.5.0",
    "lucide-react": "^0.294.0",
    "luxon": "^3.4.4",
    "next": "^14.0.3",
    "postshape": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typedoc": "^0.25.4"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/lodash.omit": "^4.5.9",
    "@types/luxon": "^3.3.6",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.3",
    "typescript": "^5.3.2"
  }
}
